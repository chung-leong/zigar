{
  "name": "rollup-plugin-zigar",
  "version": "0.11.0",
  "description": "Rollup plugin for transpiling Zig code",
  "main": "./dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "test:extended": "node ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "debug": "node --inspect-brk ./node_modules/mocha/bin/mocha.js -- test/*.test.js",
    "coverage": "node ./node_modules/c8/bin/c8.js ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chung-leong/zigar.git"
  },
  "keywords": [
    "zig",
    "rollup-plugin"
  ],
  "author": "Chung Leong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chung-leong/zigar/issues"
  },
  "homepage": "https://github.com/chung-leong/zigar#readme",
  "dependencies": {
    "zigar-compiler": "^0.11.0",
    "zigar-runtime": "^0.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "mocha-skip-if": "^1.0.4",
    "rollup": "^4.9.1",
    "vite": "^5.1.4"
  }
}
