{
  "name": "zigar-loader",
  "version": "0.12.0",
  "description": "Webpack plugin for transpiling Zig code",
  "main": "./dist/index.js",
  "type": "commonjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.mjs",
    "debug": "node --inspect-brk ./node_modules/mocha/bin/mocha.js -- test/*.test.mjs",
    "coverage": "node ./node_modules/c8/bin/c8.js ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.mjs",
    "integration-coverage": "node ./node_modules/c8/bin/c8.js ./node_modules/mocha/bin/mocha.js --parallel -- test/integration.test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chung-leong/zigar.git"
  },
  "keywords": [
    "zig"
  ],
  "author": "Chung Leong",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/chung-leong/zigar/issues"
  },
  "homepage": "https://github.com/chung-leong/zigar#readme",
  "devDependencies": {
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mocha-skip-if": "^1.0.4",
    "webpack": "^5.88.2"
  }
}
