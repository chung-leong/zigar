{
  "name": "zigar-compiler",
  "version": "0.11.0",
  "description": "Compile Zig code into JavaScript or C++ module",
  "main": "./dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=development ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "debug": "NODE_ENV=development node --inspect-brk ./node_modules/mocha/bin/mocha.js -- test/*.test.js",
    "coverage": "NODE_ENV=development node ./node_modules/c8/bin/c8.js ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "rollup": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chung-leong/zigar.git"
  },
  "keywords": [
    "zig"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./cjs": "./dist/index.cjs"
  },
  "author": "Chung Leong",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/chung-leong/zigar/issues"
  },
  "homepage": "https://github.com/chung-leong/zigar#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mocha-skip-if": "^1.0.4",
    "rollup": "^3.23.0",
    "webpack-cli": "^5.1.4"
  }
}
