{
  "name": "zigar-runtime",
  "version": "0.0.0",
  "description": "JavaScript runtime for code transpiled using Zigar",
  "main": "./dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ZIGAR_DEV=1 node ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "debug": "ZIGAR_DEV=1 node --inspect-brk ./node_modules/mocha/bin/mocha.js -- test/*.test.js",
    "coverage": "ZIGAR_DEV=1 node ./node_modules/c8/bin/c8.js ./node_modules/mocha/bin/mocha.js --parallel -- test/*.test.js",
    "rollup": "rollup -c rollup.config.js"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chung-leong/zigar.git"
  },
  "keywords": [
    "zig",
    "javascript"
  ],
  "author": "Chung Leong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chung-leong/zigar/issues"
  },
  "homepage": "https://github.com/chung-leong/zigar/#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "mersenne-twister": "^1.1.0",
    "mocha": "^10.2.0",
    "mocha-skip-if": "^1.0.4",
    "rollup": "^3.23.0"
  }
}
